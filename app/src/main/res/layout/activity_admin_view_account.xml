<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_account_info"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="edu.orangecoastcollege.cs273.vnguyen629.occlostandfound.AdminViewAccountActivity">

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/adminViewUsernameTextView"
        android:textSize="30sp"
        android:textColor="@color/cast_expanded_controller_background_color"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:text="@string/studentID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/adminViewUsernameTextView"
        android:layout_toLeftOf="@+id/adminViewUsernameTextView"
        android:layout_toStartOf="@+id/adminViewUsernameTextView"
        android:layout_marginTop="27dp"
        android:id="@+id/textView24"
        android:textSize="18sp" />

    <TextView
        android:text="@string/studentID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/adminViewUsernameTextView"
        android:layout_toLeftOf="@+id/adminViewUsernameTextView"
        android:layout_toStartOf="@+id/adminViewUsernameTextView"
        android:layout_marginTop="27dp"
        android:id="@+id/textView26"
        android:textSize="18sp" />

    <TextView
        android:text="@string/studentID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:id="@+id/textView25"
        android:textSize="18sp"
        android:layout_below="@+id/textView24"
        android:layout_alignLeft="@+id/textView24"
        android:layout_alignStart="@+id/textView24" />
</RelativeLayout>
